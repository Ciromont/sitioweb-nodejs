<%- include layouts/header.ejs %>

    <div class="container">

    <h1><%= title %></h1>


    <div class="row">


      <% for(var libro=0;libro<Libros.length;libro++) { %>

        <h3> </h3>
     


        <div class="col-sm-4">
          <div class="card">

            <img src="<%=Libros[libro].imagen%>" class="card-img-top" alt="...">
            <div class="card-body">
              <h5 class="card-title"><%=Libros[libro].titulo%> </h5>
              <p class="card-text"><%=Libros[libro].descripcion%></p>
              <h5> $ <%=Libros[libro].precio%></h5>
              
              <a href="#" class="btn btn-primary">Comprar</a>
            </div>
          </div>
        </div>
        <% } %>
      </div>

    </div>

    <%- include layouts/footer.ejs %>